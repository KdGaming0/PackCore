{
	"schemaVersion": 1,
	"id": "packcore",
	"version": "${version}",
	"name": "PackCore",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "PolyForm Perimeter License 1.0.1",
	"icon": "assets/packcore/icon.png",
	"environment": "*",
	"entrypoints": {
		"preLaunch": [
			"com.kd_gaming1.copysystem.PreLaunchInitializer"
		],
		"main": [
			"com.kd_gaming1.PackCore",
			{
				"adapter": "kotlin",
				"value": "com.kd_gaming1.PackCore"
			}
		]
	},
	"mixins": [
		"packcore.mixins.json"
	],
	"resource_packs": [
		{
			"type": "data",
			"path": "data"
		},
		{
			"type": "assets",
			"path": "assets"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.13.3+kotlin.2.1.21"
	},
	"suggests": {
		"another-mod": "*"
	}
}