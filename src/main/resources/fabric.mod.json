{
	"schemaVersion": 1,
	"id": "packcore",
	"version": "${version}",
	"name": "PackCore",
	"description": "PackCore is a companion mod for SkyBlock Enhanced modpacks, featuring a Hypixel SkyBlock-themed start menu, plus more!",
	"authors": [
		"Kd_Gaming1"
	],
	"contact": {
		"homepage": "https://modrinth.com/project/V3FnL7QV",
		"sources": "https://github.com/KdGaming0/PackCore"
	},
	"license": "PolyForm Perimeter License 1.0.1",
	"icon": "assets/packcore/icon.png",
	"environment": "*",
	"entrypoints": {
		"preLaunch": [
			"com.kd_gaming1.copysystem.PreLaunchConfigExtractor"
		],
		"main": [
			"com.kd_gaming1.PackCore",
			{
				"adapter": "kotlin",
				"value": "com.kd_gaming1.PackCore"
			}
		]
	},
	"mixins": [
		"packcore.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.13.3+kotlin.2.1.21"
	},
	"suggests": {
		"another-mod": "*"
	}
}